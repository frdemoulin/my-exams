{
  "name": "my-exams",
  "version": "1.1.11",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/frdemoulin/my-exams.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && opennextjs-cloudflare build --skipNextBuild --skipWranglerConfigCheck",
    "check:worker-size": "node -e \"const fs=require('fs');const z=require('zlib');const b=fs.readFileSync('.open-next/worker.js');const gz=z.gzipSync(b);console.log('worker.js bytes:',b.length);console.log('gzip bytes:',gz.length);console.log('gzip KiB:',(gz.length/1024).toFixed(2));console.log('limit KiB:',(3*1024).toFixed(2)); process.exit(gz.length>3*1024*1024?1:0)\"",
    "start": "next start",
    "build:next": "next build",
    "lint": "eslint . --max-warnings=0",
    "prisma:test": "ts-node scripts/test-prisma.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:dev": "ts-node scripts/seed-dev.ts",
    "db:seed:prod": "ts-node scripts/seed-prod.ts",
    "db:clear": "ts-node scripts/clear-database.ts",
    "db:reset": "npm run db:clear && npm run db:seed",
    "migrate:exercises": "ts-node scripts/migrate-to-exercises.ts",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
