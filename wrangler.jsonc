{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-exams",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-12-13",
  "compatibility_flags": ["nodejs_compat"],
  "minify": true,
  "workers_dev": true,
  "preview_urls": true,
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "my-exams"
    }
  ]
}
